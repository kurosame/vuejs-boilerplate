{
  "compilerOptions": {
    "target": "es2015",
    "module": "es2015",
    "lib": [
      "dom",
      "es5",
      "es2015.promise"
    ],
    "sourceMap": true,
    "strict": true,
    "noImplicitThis": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/ts/*"
      ]
    },
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true
  }
}